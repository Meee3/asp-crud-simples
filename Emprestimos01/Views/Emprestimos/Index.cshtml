@model IEnumerable<Emprestimos01.Models.Emprestimos>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.valorSolicitado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.valorEntrada)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.valorSolicitado)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valorEntrada)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.EmprestimoId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.EmprestimoId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.EmprestimoId">Delete</a>
                </td>
            </tr>
        }

    </tbody>
</table>
